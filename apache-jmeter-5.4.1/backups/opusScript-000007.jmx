        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="IMEI Generator With Luhn Check" enabled="true">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">public static String nextImei() {
 String str = getRandom();
 return str + getCheckDigit(str);
}

public static String getRandom() {
 long x = 10000000000000L;
 long y = 99999999999999L;
 Random r = new Random();
 long number = x + ((long) (r.nextDouble() * (y - x)));
 return new Long(number).toString();
}

public static int getCheckDigit(String str) {
 int k = luhnTest(str + &quot;0&quot;);
 int i = 0;
 if (k % 10 != 0)
  i = 10 - k % 10;
 return i;
}

public static int luhnTest(String str) {
 int sum = 0;
 boolean isEven = false;
 for (int i = str.length(); i &gt; 0; i--) {
  int k = Integer.parseInt(str.substring(i - 1, i));
  if (isEven) {
   k = k * 2;
   if (k / 10 != 0)
    k = k / 10 + k % 10;
  }

  isEven = !isEven;
  sum += k;
 }
 return sum;
}
 
vars.put(&quot;IMEI&quot;, nextImei())</stringProp>
        </BeanShellPreProcessor>
