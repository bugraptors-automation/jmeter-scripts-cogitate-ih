    }
  }
      (i+1&lt;text_question_codes.size()?jsonString.append(builder.toString()).append(&quot;,&quot;):jsonString.append(builder.toString()).append(&quot;],&quot;));
}

jsonString.append(&quot;\&quot;modelCode\&quot;:&quot;+&quot;\&quot;&quot;+vars.get(&quot;MODEL_CODE&quot;)+&quot;\&quot;&quot;+&quot;,\&quot;promotionCode\&quot;:&quot;+&quot;\&quot;&quot;+vars.get(&quot;PROMO_CODE&quot;)+&quot;\&quot;&quot;+&quot;,\&quot;returnDevice\&quot;: false,\&quot;returnDeviceThresholdValue\&quot;:&quot;+&quot;\&quot;&quot;+vars.get(&quot;BASE_PRICE&quot;)+&quot;\&quot;&quot;+&quot;,\&quot;shippingMaterialMethod\&quot;: \&quot;PRINT_OWN_LABEL\&quot;&quot;).append(&quot;},&quot;)

