              <stringProp name="script">import com.jayway.jsonpath.JsonPath;

Integer number_of_question_code = JsonPath.read(vars.get(&quot;questionData&quot;), &quot;\$.length()&quot;);

log.info(&quot;Number of questions: &quot; + number_of_question_code);

List&lt;String&gt; single_select_question_codes = JsonPath.read(vars.get(&quot;questionData&quot;), &quot;\$.[?(@.questionType==&apos;SINGLE_SELECT&apos;)].code&quot;);

List&lt;String&gt; single_select_question_ids = JsonPath.read(vars.get(&quot;questionData&quot;), &quot;\$..validAnswers[?(@.answerText == &apos;Yes&apos;)].id&quot;);

log.info(&quot;Number of select questions: &quot; + single_select_question_ids.size());

List&lt;String&gt; text_question_codes = JsonPath.read(vars.get(&quot;questionData&quot;), &quot;\$.[?(@.questionType==&apos;TEXT&apos;)].code&quot;);

List&lt;String&gt; text_question_ids = JsonPath.read(vars.get(&quot;questionData&quot;), &quot;\$..validAnswers[?(@.answerText == &apos;&apos;)].id&quot;);

log.info(&quot;Number of text questions: &quot; + text_question_codes.size());

def builder = new groovy.json.JsonBuilder()
StringBuilder jsonString = new StringBuilder(&quot;{\&quot;itemRequestDTO\&quot;:{\&quot;answers\&quot;:[&quot;);
for (int i = 0; i &lt; single_select_question_codes.size(); i++) {
    builder {
        answerSkuCode &apos;Y&apos;
        questionCode single_select_question_codes.get(i)
        textResponse &apos;Yes&apos;
    }
      jsonString.append(builder.toString()).append(&quot;,&quot;);
}

for (int i = 0; i &lt; text_question_codes.size(); i++) {
  if (text_question_codes.get(i).equals(&quot;CELL_PHONE_IMEI_ESN&quot;)) {
    builder {
      answerSkuCode &apos;TEXT&apos;
      questionCode text_question_codes.get(i)
      textResponse vars.get(&quot;IMEI&quot;)
    }
